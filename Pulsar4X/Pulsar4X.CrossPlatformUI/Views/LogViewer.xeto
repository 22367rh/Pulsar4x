  >
  <TabControl>
    <TabPage>
      <Scrollable>
        <GridView x:Name="GVevents"  DataStore="{Binding EventsDict}"  />             
      </Scrollable>
     </TabPage>
  <TabPage >
    <Scrollable >
      <GridView x:Name="GVTypeActions" DataStore="{Binding EventTypes}" />
    </Scrollable>
  </TabPage>
  </TabControl>
</Panel>